    <style name="carbon_Button" parent="@android:style/Widget.Button">
        <item name="android:gravity">center_vertical|center_horizontal</item>
        <item name="android:paddingTop">0dp</item>
        <item name="android:paddingBottom">0dp</item>
        <item name="android:paddingLeft">@dimen/carbon_padding</item>
        <item name="android:paddingRight">@dimen/carbon_padding</item>
        <item name="android:minHeight">36dp</item>
        <item name="android:minWidth">88dp</item>
        <item name="android:background">?attr/carbon_colorPrimary</item>
        <item name="carbon_elevation">@dimen/carbon_elevationLow</item>
        <item name="carbon_touchMarginTop">6dp</item>
        <item name="carbon_touchMarginBottom">6dp</item>
        <item name="carbon_cornerRadius">2dp</item>
        <item name="android:textColor">@null</item>
        <item name="android:textAppearance">?attr/carbon_textAppearance_button_inverse</item>
    </style>

    <style name="carbon_Button.Flat">
        <item name="carbon_elevation">@dimen/carbon_elevationFlat</item>
        <item name="android:background">@null</item>
        <item name="android:textColor">@null</item>
        <item name="android:textAppearance">?attr/carbon_textAppearance_button</item>
    </style>

    <style name="carbon_ToggleButton" parent="@android:style/Widget.Button.Toggle">
        <item name="android:gravity">center_vertical|center_horizontal</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:paddingLeft">@dimen/carbon_padding</item>
        <item name="android:paddingRight">@dimen/carbon_padding</item>
        <item name="android:focusable">true</item>
        <item name="android:clickable">true</item>
    </style>

    <style name="carbon_FloatingActionButton">
        <item name="carbon_cornerRadius">28dp</item>
        <item name="carbon_rippleColor">?attr/carbon_rippleColor</item>
        <item name="carbon_filterColor">?attr/carbon_iconColor</item>
        <item name="carbon_inAnimation">pop</item>
        <item name="carbon_outAnimation">pop</item>
        <item name="carbon_elevation">@dimen/carbon_elevationHigh</item>
        <item name="android:background">?attr/carbon_colorAccent</item>
        <item name="android:padding">@dimen/carbon_padding</item>
        <item name="android:minWidth">@dimen/carbon_fabSize</item>
        <item name="android:minHeight">@dimen/carbon_fabSize</item>
    </style>

    <style name="carbon_TextView" parent="@android:style/Widget.TextView">
        <item name="android:textAppearance">?attr/carbon_textAppearance_body1</item>
    </style>

    <style name="carbon_StatusBar">
        <item name="android:background">?attr/carbon_colorPrimaryDark</item>
    </style>

    <style name="carbon_NavigationBar">
        <item name="android:background">?attr/carbon_colorPrimaryDark</item>
    </style>

    <style name="carbon_SVGView">
        <item name="carbon_filterColor">?attr/carbon_iconColor</item>
        <item name="carbon_inAnimation">fade</item>
        <item name="carbon_outAnimation">fade</item>
    </style>

    <style name="carbon_FrameLayout">
        <item name="carbon_rippleColor">@null</item>
    </style>

    <style name="carbon_RelativeLayout">
        <item name="carbon_rippleColor">@null</item>
    </style>

    <style name="carbon_LinearLayout">
        <item name="carbon_rippleColor">@null</item>
    </style>

    <style name="carbon_Snackbar">
        <item name="carbon_layoutStyle">floating</item>
        <item name="carbon_inAnimation">fly</item>
        <item name="carbon_outAnimation">fly</item>
        <item name="carbon_duration">@integer/carbon_snackbarDuration</item>
    </style>

    <style name="carbon_Toolbar">
        <item name="android:background">?attr/carbon_colorPrimary</item>
        <item name="carbon_elevation">@dimen/carbon_elevationMax</item>
    </style>

    <style name="carbon_ImageView">
        <item name="carbon_rippleStyle">over</item>
        <item name="carbon_inAnimation">brightnessSaturationFade</item>
        <item name="carbon_outAnimation">brightnessSaturationFade</item>
    </style>

    <style name="carbon_ListView" parent="@android:style/Widget.ListView">
        <item name="android:divider">?attr/carbon_dividerColor</item>
        <item name="android:dividerHeight">1px</item>
    </style>

    <style name="carbon_Divider">
        <item name="android:background">?attr/carbon_dividerColor</item>
    </style>

    <style name="carbon_CircularProgress">
        <item name="carbon_indeterminate">true</item>
        <item name="carbon_arcColor">?attr/carbon_colorPrimary</item>
        <item name="carbon_arcBackground">?attr/carbon_colorBackground</item>
        <item name="carbon_arcWidth">3dp</item>
        <item name="carbon_inAnimation">progressWidth</item>
        <item name="carbon_outAnimation">progressWidth</item>
    </style>

    <style name="carbon_CardView">
        <item name="android:background">?attr/carbon_colorBackground</item>
        <item name="carbon_elevation">@dimen/carbon_elevationLow</item>
    </style>

    <style name="carbon_EditText" parent="@android:style/Widget.TextView">
        <item name="android:textAppearance">?attr/carbon_textAppearance_body1</item>
        <item name="carbon_dividerColor">?attr/carbon_colorPrimary</item>
        <item name="carbon_dividerPadding">8dp</item>
        <item name="android:background">@null</item>
    </style>
